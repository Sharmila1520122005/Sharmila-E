import java.util.*;
public class prog{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        String s = "1";
        if(n == 1){
            System.out.println(s);
        }
        for(int i = 2; i <= n; i++){
            s = s + " " + i + " " + s;
        }
        if(n != 1) System.out.print(s);
    }
}
